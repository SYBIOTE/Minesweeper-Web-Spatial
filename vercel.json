{
  "rewrites": [
    {
      "source": "/assets/(.*)",
      "destination": "/assets/$1"
    },
    {
      "source": "/icons/(.*)", 
      "destination": "/icons/$1"
    },
    {
      "source": "/manifest.json",
      "destination": "/manifest.json"
    },
    {
      "source": "/webspatial/avp/minefield",
      "destination": "/index.html"
    },
    {
      "source": "/webspatial/avp/(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/webspatial/avp",
      "destination": "/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}