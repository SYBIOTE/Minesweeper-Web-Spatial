{
  "name": "minesweeper-webspatial",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:avp": "XR_ENV=avp vite",
    "build": "tsc -b && vite build",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "lint": "eslint .",
    "preview": "vite preview",
    "run:avp": "dotenv -e .env.local -- sh -c 'webspatial-builder run --base=$XR_DEV_SERVER'",
    "build:avp": "dotenv -e .env.local -- sh -c 'webspatial-builder build --base=$XR_PRE_SERVER --bundle-id=$XR_BUNDLE_ID --teamId=$XR_TEAM_ID'",
    "publish:avp": "dotenv -e .env.local -- sh -c 'webspatial-builder publish  --base=$XR_PROD_SERVER --bundle-id=$XR_BUNDLE_ID --teamId=$XR_TEAM_ID --version=$XR_VERSION --u=$XR_DEV_NAME --p=$XR_DEV_PASSWORD'"
  },
  "dependencies": {
    "@google/model-viewer": "^4.1.0",
    "@hexafield/simple-store": "^1.0.1",
    "@react-three/drei": "^10.6.1",
    "@react-three/fiber": "^9.3.0",
    "@react-three/xr": "^6.6.20",
    "@types/three": "^0.178.1",
    "@webspatial/core-sdk": "^1.0.1",
    "@webspatial/react-sdk": "^1.0.1",
    "app-root-path": "^3.1.0",
    "dotenv": "^17.2.1",
    "i18next": "^25.4.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "lodash.debounce": "^4.0.8",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.7.2",
    "react-router-dom": "^7.9.1",
    "three": "^0.178.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.6.1",
    "@types/lodash": "^4.17.20",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "@webspatial/builder": "^1.0.2",
    "@webspatial/platform-visionos": "^1.0.1",
    "@webspatial/vite-plugin": "^0.1.9",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.4.47",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^3.2.4",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.0",
    "vite": "^7.1.0",
    "vite-plugin-html": "^3.2.2"
  }
}
